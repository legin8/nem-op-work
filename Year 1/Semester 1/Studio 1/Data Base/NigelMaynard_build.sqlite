DROP TABLE IF EXISTS user;
DROP TABLE IF EXISTS userGame;
DROP TABLE IF EXISTS console;
DROP TABLE IF EXISTS game;
DROP TABLE IF EXISTS mainGame;
DROP TABLE IF EXISTS dlc;
DROP TABLE IF EXISTS userConsole;
DROP TABLE IF EXISTS consoleGame;
DROP TABLE IF EXISTS series;
DROP TABLE IF EXISTS internet;
DROP TABLE IF EXISTS gameSeries;

-- This is where I create the tables
-- user table main
CREATE TABLE user (
    user_id INTEGER NOT NULL PRIMARY KEY,
    username TEXT
);
----------------------------------------------
-- joining table user to console
CREATE TABLE userConsole (
    user_id INTEGER,
    console_id INTEGER,
    FOREIGN KEY (user_id) REFERENCES user (user_id),
    FOREIGN KEY (console_id) REFERENCES console (console_id)
);
-----------------------------------------------
-- console table main
CREATE TABLE console (
    console_id INTEGER NOT NULL PRIMARY KEY,
    user_id INTEGER,
    ps4 TEXT,
    ps_vita TEXT,
    ps3 TEXT,
    FOREIGN KEY (console_id) REFERENCES userConsole (user_id)
);
------------------------------------------------
-- joining table game to user
CREATE TABLE userGame (
    user_id INTEGER,
    game_id INTEGER,
    FOREIGN KEY (user_id) REFERENCES user (user_id),
    FOREIGN KEY (game_id) REFERENCES game (game_id)
);
---------------------------------------------------
-- joining table console to game
CREATE TABLE consoleGame (
    game_id INTEGER,
    console_id INTEGER,
    FOREIGN KEY (console_id) REFERENCES console (console_id),
    FOREIGN KEY (game_id) REFERENCES game (game_id)
);
---------------------------------------------------------
---------------------------------------------------------
---------------------------------------------------------
-- game table main
CREATE TABLE game (
    game_id INTEGER NOT NULL PRIMARY KEY,
    game_name TEXT
);
---------------------------------------------------------
---------------------------------------------------------
---------------------------------------------------------
-- mainGame table main
CREATE TABLE mainGame (
    mainGame_id INTEGER NOT NULL PRIMARY KEY,
    user_id INTEGER NOT NULL,
    game_id INTEGER,
    game_name TEXT,
    trophy_lvl INTEGER,
    trophy_name TEXT,
    trophy_date TEXT,
    FOREIGN KEY (game_id) REFERENCES game (game_id)
);
-------------------------------------------------------------
-- dlc table main
CREATE TABLE dlc (
    dlc_id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    user_id INTEGER NOT NULL,
    game_id INTEGER,
    game_name TEXT,
    dlc_name TEXT,
    trophy_lvl INTEGER,
    trophy_name TEXT,
    trophy_date TEXT,
    FOREIGN KEY (game_id) REFERENCES game (game_id)
);
-----------------------------------------------------------------
-- joining table game to series
CREATE TABLE gameSeries (
game_id INTEGER,
series_id INTEGER,
FOREIGN KEY (game_id) REFERENCES game (game_id),
FOREIGN KEY (series_id) REFERENCES series (series_id)
);
-----------------------------------------------------------------
-- series table main
CREATE TABLE series (
    series_id INTEGER PRIMARY KEY AUTOINCREMENT,
    game_id INTEGER,
    game_series TEXT

);
-------------------------------------------------------------------
CREATE TABLE internet (
    onlinePlay_ID INTEGER,
    game_id INTEGER,
    hasOnline TEXT,
    onlineWorks TEXT,
    FOREIGN KEY (onlinePlay_ID) REFERENCES game (game_id)
);










